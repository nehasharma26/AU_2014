<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="assignment.hibernate.enteties">
 <class name="Institute" table="institute">
 <meta attribute="class-description">
 This class contains the institute detail. 
 </meta>
 <id name="instId" type="int" column="instId">
 <generator class="native"/>
 </id>
 <property name="title" column="title" type="string"/>
 <property name="description" column="description" type="string"/>
 <property name="location" column="location" type="string"/>
 <property name="branches" column="branches" type="string"/>
 <property name="image" column="image" type="string"/>
  <property name="courses" column="courses" type="string"/>
 </class>
</hibernate-mapping>
